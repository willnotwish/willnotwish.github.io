{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::b1.nickadams.co.uk",
        "arn:aws:s3:::b1.nickadams.co.uk/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::b2.nickadams.co.uk",
        "arn:aws:s3:::b2.nickadams.co.uk/*"
      ]
    }
  ]
}